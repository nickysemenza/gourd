    "typescript": "5.0.2",
    "universal-cookie": "4.0.4",
    "wasm-loader": "1.3.0"
  },
  "scripts": {
    "build:tailwind": "tailwind build -i src/tailwind.css -o src/tailwind.output.css",
    "watch:tailwind": "tailwind -i ./src/tailwind.css -o src/tailwind.output.css --watch",
    "prebuild": "run-s build:tailwind",
    "start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
    "x": "x",
    "start:react": "REACT_APP_API_URL=http://localhost:4242 craco -r @cypress/instrument-cra start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "generate-fetcher": "restful-react import --file ../internal/api/openapi.yaml --output src/api/openapi-hooks/api.tsx",
    "postgenerate-fetcher": "prettier src/api/openapi-hooks/api.tsx --write",
    "coverage:unit": "craco test --watchAll=false --coverage --coverageDirectory=coverage/unit",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "start:integration": "BROWSER=none PORT=3001 craco -r @cypress/instrument-cra start",
    "cy:run:integration": "cypress run --record --key 265312e3-7b8c-4031-ad0a-5f825b95cfce",
    "test:integration": "start-server-and-test start:integration http://localhost:3001 cy:run:integration"
  },
  "eslintConfig": {
    "extends": "react-app",
    "ignorePatterns": [
      "src/wasm/*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "path": "path-browserify"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.10.0",
    "@cypress/instrument-cra": "1.4.0",
    "@openapitools/openapi-generator-cli": "2.6.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "autoprefixer": "10.4.13",
    "chokidar-cli": "3.0.0",
    "cypress": "12.12.0",
    "cypress-terminal-report": "5.1.1",
    "ibm-openapi-validator": "1.0.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "prettier": "2.8.3",
    "start-server-and-test": "2.0.0",
    "tailwindcss": "3.3.0"
  }
}
