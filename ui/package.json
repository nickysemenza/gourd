    "bezier-js": "4.0.3"
  },
  "scripts": {
    "build:tailwind": "tailwind build src/tailwind.css -i src/tailwind.css -o src/tailwind.output.css",
    "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/tailwind.output.css -c 'npm run build:tailwind'",
    "prebuild": "run-s build:tailwind",
    "start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
    "x": "x",
    "start:react": "REACT_APP_API_URL=http://localhost:4242 craco -r @cypress/instrument-cra start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "generate-fetcher": "restful-react import --file ../api/openapi.yaml --output src/api/openapi-hooks/api.tsx",
    "postgenerate-fetcher": "prettier src/api/openapi-hooks/api.tsx --write",
    "coverage:unit": "craco test --watchAll=false --coverage --coverageDirectory=coverage/unit",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "start:integration": "BROWSER=none PORT=3001 craco -r @cypress/instrument-cra start",
    "cy:run:integration": "cypress run --record --key 265312e3-7b8c-4031-ad0a-5f825b95cfce",
    "test:integration": "start-server-and-test start:integration http://localhost:3001 cy:run:integration"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "path": "path-browserify"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.12",
    "@cypress/instrument-cra": "1.4.0",
    "@openapitools/openapi-generator-cli": "2.4.18",
    "@testing-library/jest-dom": "5.16.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "autoprefixer": "10.4.0",
    "chokidar-cli": "3.0.0",
    "cypress": "9.1.1",
    "cypress-terminal-report": "3.4.0",
    "esbuild": "0.14.2",
    "ibm-openapi-validator": "0.51.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.4",
    "prettier": "2.5.1",
    "start-server-and-test": "1.14.0",
    "tailwindcss": "2.2.19"
  }
