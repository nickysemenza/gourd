  "dependencies": {
    "@cypress/instrument-cra": "^1.4.0",
    "@sentry/browser": "^5.29.2",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.19",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^14.14.16",
    "@types/query-string": "^6.3.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-select": "^3.0.28",
    "@types/react-table": "^7.0.26",
    "apollo-boost": "^0.4.7",
    "blurhash": "^1.1.3",
    "core-js": "^3.8.1",
    "dayjs": "^1.9.8",
    "graphql": "^15.4.0",
    "ibm-openapi-validator": "^0.33.0",
    "immutability-helper": "^3.0.2",
    "jwt-decode": "^3.1.2",
    "parse-duration": "^0.4.4",
    "query-string": "^6.13.8",
    "react": "^17.0.1",
    "react-blurhash": "^0.1.3",
    "react-cookie": "^4.0.3",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-google-login": "^5.2.1",
    "react-hotkeys-hook": "^3.0.0",
    "react-json-view": "^1.19.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.1",
    "react-select": "^3.1.1",
    "react-table": "^7.6.2",
    "react-toastify": "^6.2.0",
    "redoc": "^2.0.0-rc.40",
    "restful-react": "^15.1.3",
    "styled-components": "^5.2.1",
    "typescript": "4.1.3",
    "universal-cookie": "^4.0.4"
  },
  "scripts": {
    "build:tailwind": "tailwind build src/tailwind.css -o src/tailwind.output.css",
    "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/tailwind.output.css -c 'npm run build:tailwind'",
    "prebuild": "run-s build:tailwind",
    "start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
    "x": "x",
    "start:react": "REACT_APP_API_URL=http://localhost:4242 react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate-fetcher": "restful-react import --file ../api/openapi.yaml --output src/api/openapi-hooks/api.tsx",
    "postgenerate-fetcher": "prettier src/api/openapi-hooks/api.tsx --write",
    "coverage:unit": "react-scripts test --watchAll=false --coverage --coverageDirectory=coverage/unit",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "start:integration": "BROWSER=none PORT=3001 react-scripts -r @cypress/instrument-cra start",
    "cy:run:integration": "cypress run --record --key 265312e3-7b8c-4031-ad0a-5f825b95cfce",
    "test:integration": "start-server-and-test start:integration http://localhost:3001 cy:run:integration"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.8.8",
    "@openapitools/openapi-generator-cli": "^2.1.16",
    "autoprefixer": "^10.1.0",
    "chokidar-cli": "^2.1.0",
    "cypress": "^6.2.0",
    "cypress-terminal-report": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.2",
    "prettier": "^2.2.1",
    "redoc-cli": "^0.10.2",
    "start-server-and-test": "^1.11.7",
    "tailwindcss": "^2.0.2"
  }
}
