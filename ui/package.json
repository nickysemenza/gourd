    "dayjs": "1.11.7",
    "graphviz-react": "1.2.5",
    "highcharts": "11.0.0",
    "highcharts-react-official": "3.2.0",
    "immutability-helper": "3.1.1",
    "jwt-decode": "3.1.2",
    "parse-duration": "1.0.2",
    "path-browserify": "1.0.1",
    "pluralize": "8.0.0",
    "query-string": "8.1.0",
