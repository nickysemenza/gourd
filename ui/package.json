{
  "name": "gourd-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-components": "^3.1.5",
    "@apollo/react-hoc": "^3.1.5",
    "@apollo/react-hooks": "^3.1.5",
    "@cypress/instrument-cra": "^1.1.1",
    "@sentry/browser": "^5.18.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^12.0.0",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.4",
    "@types/react-select": "^3.0.19",
    "@types/react-table": "^7.0.16",
    "apollo-boost": "^0.4.7",
    "blurhash": "^1.1.3",
    "core-js": "^3.6.5",
    "dayjs": "^1.9.1",
    "graphql": "^15.0.0",
    "ibm-openapi-validator": "^0.29.3",
    "immutability-helper": "^3.0.2",
    "jwt-decode": "^3.0.0",
    "query-string": "^6.13.2",
    "react": "^16.13.1",
    "react-blurhash": "^0.1.2",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-google-login": "^5.1.21",
    "react-hotkeys-hook": "^2.1.3",
    "react-json-view": "^1.19.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.3",
    "react-select": "^3.1.0",
    "react-table": "^7.5.1",
    "redoc": "^2.0.0-rc.40",
    "restful-react": "^14.5.1",
    "typescript": "^3.9.7",
    "universal-cookie": "^4.0.4"
  },
  "scripts": {
    "build:tailwind": "tailwind build src/tailwind.css -o src/tailwind.output.css",
    "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/tailwind.output.css -c 'npm run build:tailwind'",
    "prebuild": "run-s build:tailwind",
    "start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
    "x": "x",
    "start:react": "REACT_APP_API_URL=http://localhost:4242 react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "graphql-codegen --config codegen.yml",
    "generate-fetcher": "restful-react import --file ../api/openapi.yaml --output src/api/openapi-hooks/api.tsx",
    "postgenerate-fetcher": "prettier src/api/openapi-hooks/api.tsx --write",
    "coverage:unit": "react-scripts test --watchAll=false --coverage --coverageDirectory=coverage/unit",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "start:integration": "BROWSER=none PORT=3001 react-scripts -r @cypress/instrument-cra start",
    "cy:run:integration": "cypress run",
    "test:integration": "start-server-and-test start:integration http://localhost:3001 cy:run:integration"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.5.3",
    "@graphql-codegen/cli": "^1.13.2",
    "@graphql-codegen/introspection": "1.13.2",
    "@graphql-codegen/typescript": "1.13.2",
    "@graphql-codegen/typescript-document-nodes": "1.13.2",
    "@graphql-codegen/typescript-graphql-files-modules": "1.13.2",
    "@graphql-codegen/typescript-operations": "1.13.2",
    "@graphql-codegen/typescript-react-apollo": "1.13.2",
    "@openapitools/openapi-generator-cli": "^2.0.3",
    "chokidar-cli": "^2.1.0",
    "cypress": "^4.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "redoc-cli": "^0.9.12",
    "start-server-and-test": "^1.11.0",
    "tailwindcss": "^1.8.4"
  }
}
